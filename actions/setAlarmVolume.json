    "caption": "Set alarm volume",
    "form": [
        "for ($i=0; $i<10; $i++) {",
        "   $Options[] = [",
        "       'caption'   =>  'Alarm '.($i+1),",
        "       'value'     =>  $i",
        "   ];",
        "}",
        "$Form[]=['type' => 'Select','name'=>'INDEX','caption'=>'Alarm','options'=>$Options];",
        "$Form[]=['type' => 'NumberSpinner','name'=>'VALUE','caption'=>'Volume','suffix'=>' %','minimum'=>0,'maximum'=>100];",
        "return $Form;"
    ],
